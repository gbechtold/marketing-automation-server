# Activepieces Environment Variables

# Security keys (generate secure random strings)
AP_ENCRYPTION_KEY=generate_32_character_random_string
AP_JWT_SECRET=generate_32_character_random_string

# Frontend URL (must match your domain)
AP_FRONTEND_URL=https://automation.yourdomain.com

# Database configuration
AP_POSTGRES_HOST=postgres
AP_POSTGRES_PORT=5432
AP_POSTGRES_DATABASE=activepieces
AP_POSTGRES_USERNAME=activepieces
AP_POSTGRES_PASSWORD=your_secure_postgres_password

# Redis configuration
AP_REDIS_HOST=redis
AP_REDIS_PORT=6379

# Features
AP_SIGN_UP_ENABLED=true